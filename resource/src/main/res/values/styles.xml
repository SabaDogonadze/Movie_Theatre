<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomCheckBoxStyle" parent="Widget.MaterialComponents.CompoundButton.CheckBox">
        <item name="android:buttonTint">@color/white</item>
        <item name="materialThemeOverlay">@style/CheckBoxThemeOverlay</item>
    </style>

    <style name="CheckBoxThemeOverlay">
        <item name="colorControlActivated">@color/white</item>
        <item name="colorOnSurface">@color/white</item>
    </style>

    <style name="CircularImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.TopRoundedCard" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">12dp</item>
        <item name="cornerSizeTopRight">12dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>
</resources>